<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             Title="Configurar"
             x:Class="TrabalhoEMC0101.Views.ConfigurarURL">
    <ContentPage.Content>
        <StackLayout>
            <Entry
                    x:Name="urlLayout"
                   
                    Margin="25,10,25,10"
                    BackgroundColor="White"
                    />
            <Button 
                    Clicked="Button_Clicked"
                    BackgroundColor="#77dd77"
                    Text="Configurar URL"
                    Margin="25,15,25,10"
                    />
        </StackLayout>
    </ContentPage.Content>
</ContentPage>